{
  "name": "calebmasters",
  "version": "1.0.0",
  "description": "Obligatory portfolio for Caleb Masters",
  "main": "index.html",
  "scripts": {
    "watch": "sass --no-source-map --style=compressed -w source:static",
    "compile" : "sass --no-source-map --style=compressed source:static",
    "dist": "npm run compile && npm run buildNext",
    "buildNext": "cd cmnext && npm run build",
    "dev": "concurrently \"npm run watch\" \"serve -s .\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecaleblee/calebmasters.git"
  },
  "keywords": [
    "Developer",
    "portfolio"
  ],
