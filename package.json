  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thecaleblee/calebmasters/issues"
  },
  "homepage": "https://github.com/thecaleblee/calebmasters#readme",
  "devDependencies": {
    "concurrently": "^6.1.0",
    "create-next-app": "^10.2.1",
    "sass": "^1.32.13",
    "serve": "^11.3.2"
  }
}
